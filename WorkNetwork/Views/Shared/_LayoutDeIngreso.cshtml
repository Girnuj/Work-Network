<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Ingreso"] - Ingreso</title>

	<link rel="stylesheet" href="~/css/Ingreso.css" />
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
</head>

<body>
	<section class="cont s--inactive">
		<!-- cont inner start -->
		<div class="cont__inner">
			<!-- el start -->
			<div class="el">
				<div class="el__overflow">
					<div class="el__inner">
						<div class="el__bg"></div>
						<div class="el__preview-cont"></div>
						<div class="el__content">
							<div class="el__text">Empresa</div>
							<!-- /////form/////////form//////////////form///////////////////////////////////////////////////// -->
							<div class="main12">
								<div class="container12 a-container" id="a-container">
									<form class="form12">
										<h2 class="form_title title12">Crear Cuenta</h2>
										<div class="form__icons"><img class="form__icon"
												src="data:image/svg+xml;base64,"
												alt=""><img class="form__icon"
												src="data:image/svg+xml;base64,"><img
												class="form__icon"
												src="data:image/svg+xml;base64,">
										</div><span class="form__span">o use su Email para registrarse</span>
										<input type="hidden" id="Rol" value="Empresa">
										<input class="form__input" type="text" placeholder="Correo electronico"
											id='Email'>
										<input class="form__input" type="password" placeholder="Contraseña"
											id='Password'>
										<input class="form__input" type="password" placeholder="Confirmar la contraseña"
											id='ConfirmPassword'>
										<button class="form12__button button12 submit12" onclick='register()'
											id="registrarEmpresa">Empezar Ya!</button>

									</form>
								</div>
								<div class="container13 b-container" id="b-container">
									<form class="form13" id="" method="" action="">
										<h2 class="form_title title12">Iniciar Sesión</h2>
										<div class="form__icons"><img class="form__icon"
												src="data:image/svg+xml;base64,"
												alt=""><img class="form__icon"
												src="data:image/svg+xml;base64,"><img
												class="form__icon"
												src="data:image/svg+xml;base64,">
										</div><span class="form__span">o use su cuenta de Email</span>
										<input class="form__input" type="text" id='correoEmpresa' placeholder="Email">
										<input class="form__input" type="password" id='passwordEmpresa'
											placeholder="Password"><a class="form__link">¿Olvido su contraseña?</a>
										<button class="form12__button button12 submit12" onclick='loginEmpresa()'
											id="iniciarSesionEmpresa">Ingresar</button>
									</form>
								</div>
								<div class="switch" id="switch-cnt">
									<div class="switch__circle"></div>
									<div class="switch__circle switch__circle--t"></div>
									<div class="switch__container" id="switch-c1">
										<h2 class="switch__title title12">Bienvenido!!</h2>
										<p class="switch__description description12">Para mantenerse conectado con
											nosotros, inicie sesión con su información personal</p>
										<button class="switch__button button12 switch-btn">Iniciar Sesion</button>
									</div>
									<div class="switch__container is-hidden" id="switch-c2">
										<h2 class="switch__title title12">Hola Amigo !</h2>
										<p class="switch__description description12">Ingresa tus datos personales y
											comienza tu viaje con nosotros</p>
										<button class="switch__button button12 switch-btn">Registrarse</button>
									</div>
								</div>
							</div>
							<!-- /////fin form/////////////end form//////////////fin form/////////////end form.//////////////////////////////////// -->
							<div class="el__close-btn"></div>
						</div>
					</div>
				</div>

				<div class="el__index">
					<div class="el__index-back">
						<h2 class="titulo">Empresa</h2>
						<p class="texto">Para Empresas o Personas que quieran postular una vacante</p>
					</div>
				</div>

			</div>
			<!-- //////////la otra card//////////////la otra card//////////////////la otra card///////////////////////////////////////////////////////////////////////////////// -->
			<div class="el">
				<div class="el__overflow">
					<div class="el__inner">
						<div class="el__bg"></div>
						<div class="el__preview-cont"></div>
						<div class="el__content">
							<div class="el__text">Persona</div>
							<div class="main12">
								@* FORMULARIO DE REGISTRO *@
								<div class="container12 a-container" id="a-container12">
									<form class="form12" id="a-form">
										<h2 class="form_title title12">Crear Cuenta</h2>
										<div class="form__icons"><img class="form__icon"
												src="data:image/svg+xml;base64,"
												alt=""><img class="form__icon"
												src="data:image/svg+xml;base64,"><img
												class="form__icon"
												src="data:image/svg+xml;base64,">
										</div><span class="form__span">o use su Email para registrarse</span>
										<input type="hidden" id="Persona" value="Usuario">
										<span class="text-danger visually-hidden" id="errorRegisterPersona">asdfasdfasd</span>
										<input class="form__input" type="text" placeholder="Correo electronico"
											id='EmailPersona'>
										<input class="form__input" type="password" placeholder="Contraseña"
											id='PasswordPersona'>
										<input class="form__input" type="password" placeholder="Confirmar la contraseña"
											id='ConfirmPasswordPersona'>
										<button class="form12__button button12 submit12" onclick='registerPerson()'
											id="registrarPersona">Empezar Ya!</button>
										<div id="loaderRegisterPersona" class="visually-hidden spinner">
											<span></span>
											<span></span>
											<span></span>
										</div>
									</form>
								</div>
								<div class="container13 b-container" id="b-container12">
									<form class="form13" id="b-form" method="" action="">
										<h2 class="form_title title12">Iniciar Sesión</h2>
										<div class="form__icons"><img class="form__icon"
												src="data:image/svg+xml;base64,"
												alt=""><img class="form__icon"
												src="data:image/svg+xml;base64,"><img
												class="form__icon"
												src="data:image/svg+xml;base64,">
										</div><span class="form__span">o use su cuenta de Email</span>
										<span class="text-danger visually-hidden" id="errorInicioSesion">Contraseña o usuario invalido</span>
										<input class="form__input" type="text" placeholder="Email" id="correoPersona">
										<input class="form__input" type="password" placeholder="Password"
											id="passwordPersona">
										<a class="form__link">¿Olvido su contraseña?</a>
										<button class="form12__button button12 submit12" asp-area="Identity"
											onclick='loginPersona()' asp-page="/Account/Login"
											id="iniciarSesionPersona">Ingresar</button>
										<div id="loaderLoginPersona" class="visually-hidden spinner">
											<span></span>
											<span></span>
											<span></span>
										</div>
									</form>
								</div>
								<div class="switch" id="switch-cnt12">
									<div class="switch__circle"></div>
									<div class="switch__circle switch__circle--t"></div>
									<div class="switch__container" id="switch-c12">
										<h2 class="switch__title title12">Bienvenido!!</h2>
										<p class="switch__description description12">Para mantenerse conectado con
											nosotros, inicie sesión con su información personal</p>
										<button class="switch__button button12 switch-btn">Iniciar Sesión</button>
									</div>
									<div class="switch__container is-hidden" id="switch-c23">
										<h2 class="switch__title title12">Hola Amigo !</h2>
										<p class="switch__description description12">Ingresa tus datos personales y
											comienza tu viaje con nosotros</p>
										<button class="switch__button button12 switch-btn">Registrarse</button>
									</div>
								</div>
							</div>
							<!-- /////fin form/////////////end form//////////////fin form/////////////end form.//////////////////////////////////// -->
							<div class="el__close-btn"></div>

						</div>
					</div>
				</div>
				<div class="el__index">
					<div class="el__index-back2">
						<h2 class="titulo2">Persona</h2>
						<p class="texto2">Para Personas que quieran postularse a una vacante</p>
					</div>
				</div>

			</div>
			<!-- el end -->
		</div>
		<!-- cont inner end -->
	</section>
	<div class="box2">

		<h2 class="text1"> Work NetWork </h2>
		<h2 class="text2"> ¿Quienes somos? </h2>
		<h4 class="text3">Work NetWork es un portal de empleo dedicada a aquellas personas en busca de trabajo y para
			aquellas empresas que buscan empleados...</h4>
		<img src="imagen5.svg" alt="imagen4" class="logo3" height="300px" width="400px">

	</div>
	<div class="box3">
		<h2 class="text4"> Nuestra Misión </h2>
		<!-- <h2 class="text5"> Quienes somos? </h2> -->
		<h4 class="text6">Comprometidos con la sociedad y valorando el trabajo...</h4>
		<!-- <img src="imagen5.svg" alt="imagen4" class="logo3" height="300px" width="400px"> -->
	</div>

</body>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
	crossorigin="anonymous">
	</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.10.4/gsap.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.10.4/ScrollTrigger.min.js"></script>
<script src="~/js/ingreso.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
<script src="~/js/GestionDeUsuarios.js"></script>

</html>